<NotepadPlus>
    <UserLang name="Supernova" ext="nova">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="no" commentLine="no" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">([0)]0</Keywords>
            <Keywords name="Folder+">doif dowhile procedure function&#x000D;&#x000A;</Keywords>
            <Keywords name="Folder-">endif endwhile endprocedure endfunction</Keywords>
            <Keywords name="Operators">- % ( ) * / [ ] { } + &lt; = &gt;</Keywords>
            <Keywords name="Comment">1 2 0:note:</Keywords>
            <Keywords name="Words1">i you please want see contains is maximize minimize centre show hide close additem equal to from use add memory file dll function call with parameters window button label picture textbox editbox listbox combobox checkbox progressbar datepicker spinner grid browse slider treeview tabs tabpage&#x000D;&#x000A;frame hyperlink status bar item date clock keyboard menu popup item application counter title back color direction top left width height name font size fore bold italic underline strickout transparent value caption must range minimum maximum address note start end step parent the and set auto off on instructions are end of done selected ok store get events while if decimal places allowed execute resize modal not or library com object create type method property&#x000D;&#x000A;</Keywords>
            <Keywords name="Words2"></Keywords>
            <Keywords name="Words3"></Keywords>
            <Keywords name="Words4"></Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="FF0080" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="FF8040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="808040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="400040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="408080" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="0080C0" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="408080" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
